{
    "luci-app-wrtbwmon-ng": {
        "description": "Grant access to LuCI app wrtbwmon (NG)",
        "read": {
            "file": {
                "/tmp/usage.db": ["read"],
                "/etc/wrtbwmon.user": ["read"],
                "/usr/sbin/wrtbwmon": ["exec"],
                "/bin/cp": ["exec"],
                "/etc/luci-wrtbwmon.conf": ["read"]
            },
            "uci": ["wrtbwmon-ng", "luci-app-wrtbwmon-ng"]
        },
        "write": {
            "file": {
                "/bin/cat": ["exec"],
                "/bin/rm": ["exec"],
                "/bin/cp": ["exec"],
                "/etc/luci-wrtbwmon.conf": ["write"],
                "/etc/wrtbwmon.user": ["write"]
            },
            "uci": ["wrtbwmon-ng", "luci-app-wrtbwmon-ng"]
        }
    }
}